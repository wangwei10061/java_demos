import java.util.Scanner;
public class SwitchTest 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		switch(str)
		{
			case "add"： System.out.println("进行添加操作"); break;
			case "delete":	 System.out.println("进行删除操作");break;
			case "update":	 System.out.println("进行更新操作");break;
			default : System.out.println("进行查询操作");break;
		}
	}
}
